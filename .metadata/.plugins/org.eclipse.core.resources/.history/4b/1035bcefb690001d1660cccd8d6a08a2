package com.lcpan.m03;

public class Unity {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int x = 3;
		int y = 4 ,h,z; 
	
		z = ++x * y;
		System.out.println("x= "+x+",  y= "+y+",  z= "+z);  
		h= x++ * y;
		System.out.println("x= "+x+",  y= "+y+",  h= "+h); 


	}

}
