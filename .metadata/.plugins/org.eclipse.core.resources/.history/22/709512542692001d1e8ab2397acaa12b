package com.lcpan.m04;

public class MaxMin {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int[] num = new int[] { 2, 5, 4, 6, 8, 7, 2, 1, 9, 3 };
		
		for (int j = 0; j < num.length - 1; j++) {
			if (num[j] > num[j+1]) {
				int temp = num[j];
				num[j]= num[j+1];
				num[j+1] = temp;
				
			}
			System.out.print(num[j]);
			
		}

	}

}
