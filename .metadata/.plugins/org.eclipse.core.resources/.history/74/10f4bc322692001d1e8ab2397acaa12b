package com.lcpan.m04;

public class MaxMin {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int[] num = new int[] { 2, 5, 4, 6, 8, 7, 2, 1, 9, 3 };
		int i = num[0];
		for (int j = 1; j < num.length - 1; j++) {
			if (i > num[j]) {
				int temp = i;
				i = num[j];
				num[j] = temp;
				
			}
			System.out.println(num[j]);
			
		}

	}

}
