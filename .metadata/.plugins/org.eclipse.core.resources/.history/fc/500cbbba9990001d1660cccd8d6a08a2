package com.lcpan.m02;

public class Literal {
	public static void main(String[] args) {
		int x = 10;
		System.out.println(x);

		float f = 3.0F; // 必加上F
		System.out.println(f);

		double d = 3.6;
		System.out.println(d);

		long l = 20L; // 有無L沒差
		System.out.println(l);
		
		
		
		String s = "Hello World";
		System.out.println(s);

	}
}