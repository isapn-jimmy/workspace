package com.lcpan.m04;

public class ThreeDimArray{
	public static void main(String[] args) {
		int sum = 0;
		int[][] arr = { { 2, 6, 1 }, { 4, 3, 5 } };
		// �[�`arr�U��������
		for (int i = 0; i < arr.length; i++)
			for (int j = 0; j < arr[i].length; j++)
				for (z=0; z <= )
				sum += arr[i][j];
		System.out.println("sum = " + sum);
	}
}
